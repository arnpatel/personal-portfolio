<style>
    input {
      padding: 17px;
      border: 1px solid #ccc;
      border-radius: 3px;
      margin-bottom: 10px;
      width: 100%;
      box-sizing: border-box;
      color: #2C3E50;
      font-size: 13px;
    }
    img {
    display: block;
    margin: auto;
  }
  
  .message1 {
    margin-top: 20px;
    margin-bottom: 20px;
  }
  .hide{opacity:0; display: none; height: 0; padding: 0;}
  .centered {
    font-size: 90%;
    color: rgb(189, 17, 17);
    text-align: left;
    padding-left: 20px;
  }
  .success1 {
    color: green;
  }
  
  .para{
    line-height: 5px;
  }
  
  </style>



<div style="text-align: center; border: 1px solid  white; margin: 0 auto; width: 43%; padding: 20px; border-radius: 10px; box-shadow: 0 0 5px  #575957;">

    <script src="https://www.google.com/recaptcha/api.js?render=6LdtcuYZAAAAAIwojPSAgoVHOH8ePgJahEXva5rP&onload=onLoad&render=explicit"></script>
  
  <p><b>Reset Your Password</b></p>


  <img src="data:image/png;base64," style="max-width: 180px; padding-left: 40px;">

  
  
        <div style="text-align: center;">


            <p>Create new password for your account:</p>

            <% include ./partials/messages %><br>
        
            <form action="/auth/reset/<%=token%>" method="POST">
                <input id="reCaptcha" type="hidden" name="g-recaptcha-response">
                <input type="password" name="password" placeholder="New password (required)" class="" id="password">
                <div class="centered" style="width:400px">
                    <p style="color: #234d6b">Realtime password guidelines:</p>
                      <p class="lowercase-char para">At least one lowercase character</p>
                      <p class="uppercase-char para">At least one uppercase character</p>
                      <p class="number-char para">At least one number</p>
                      <p class="special-char para">At least one special character</p>
                      <p class="8-char para">At least 8 characters</p>
                      <p class="success1 hide para">The password works, go take over the world.</p>
                    </div>
                <input type="password" name="confirm" placeholder="Confirm new password (required)"><br>
            <input type="submit" style="width:100%; border-radius: 25px; padding: 12px;" onclick="change()" type="button" value="Reset" id="myButton1"></input>
            </form>
  
              <br>
  
        <p>Remember your password? <br>
          <input onclick="location.href = '/auth/login';" type="submit" value="Sign in" style="width:100%; border-radius: 25px; background-color: white; color: black; border: 1.7px solid black; padding: 10px;" id="myButton1"/>
  
    </div>
  </div>
  
  
  
  
  <script>
    function onLoad()  {
        grecaptcha.ready(function() {
            grecaptcha.execute(`6LdtcuYZAAAAAIwojPSAgoVHOH8ePgJahEXva5rP`, {action: 'submit'}).then(function(token) {
                let t = document.querySelector('#reCaptcha')
                t.value = token
            });
        }); 
    }
  </script>
  
  
  <style>
  button:hover {
    background-color: yellow;
  }
  </style>
  
  
  <script>
    function change()
  {
      var elem = document.getElementById("myButton1");
      if (elem.value=="Reset") {
        elem.value = "Processing...";
          elem.style.backgroundColor = "#687a82";
      }
  }
  </script>



